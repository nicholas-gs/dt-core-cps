<?xml version='1.0'?>
<launch>
    <arg name="veh"/>
    <arg name="param_file_name" default="default"/>

    <node pkg="anti_instagram" exec="anti_instagram_node" name="anti_instagram_node" 
        namespace="$(var veh)">
        <param name="veh" value="$(var veh)"/>
        <param name="param_file_path" 
            value="$(find-pkg-share anti_instagram)/config/$(var param_file_name).yaml"/>
    </node>
</launch>
